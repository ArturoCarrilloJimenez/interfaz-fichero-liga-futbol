<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar style="-fx-background-color: #9278D3;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jv.interfazligafutbol.NavController">
   <menus>
      <Menu mnemonicParsing="false" text="Inicio">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#index" text="Ir al inicio" />
         </items></Menu>
      <Menu mnemonicParsing="false" text="Equipo">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#addEquipo" text="Nuevo" />
            <MenuItem mnemonicParsing="false" onAction="#mostrarEquipo" text="Mostar datos" />
            <MenuItem mnemonicParsing="false" onAction="#delateEquipo" text="Borrar" />
            <MenuItem mnemonicParsing="false" onAction="#modificarEquipo" text="Modificar" />
         </items></Menu>
      <Menu mnemonicParsing="false" text="Ordenar">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#orderAsc" text="Ascenedente" />
            <MenuItem mnemonicParsing="false" onAction="#orderDesc" text="Descendente" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Buscar">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#buscar" text="Equipo" />
         </items></Menu>
   </menus>
</MenuBar>
